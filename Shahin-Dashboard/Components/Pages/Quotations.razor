@page "/quotations"

<!-- Link the CSS file -->
<link href="css/qu.css" rel="stylesheet">

<!-- Page Title -->
<h1 class="page-title">Quotations</h1>

<!-- Quotations Section -->
<section class="quotations-box" aria-labelledby="quotations-title">
    <div class="actions-container">
        <!-- Action Buttons -->
        <div class="button-group">
            <button class="action-button" type="button">
                <img src="assets/img/pol2.svg" alt="Issue Policy Offer" class="nav-icon" aria-hidden="true">
                Issue Policy Offer
            </button>
            <button class="action-button2" type="button">
                <img src="assets/img/pol3.svg" alt="Members Add Offer" class="nav-icon" aria-hidden="true">
                Members Add Offer
            </button>
        </div>

        <!-- Empty State -->
        <div class="empty-state" role="status">
            You Have No Active Quotes
        </div>
    </div>
</section>
